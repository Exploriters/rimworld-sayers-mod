<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>Make_PlantElement</defName>
    <label>提炼植元素</label>
    <description>从植物——尤其是木材中提炼植元素。</description>
    <jobString>正在提炼植元素</jobString>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Cremate</soundWorking>
    <workAmount>1000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>35</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <PlantElement>17</PlantElement>
    </products>
    <researchPrerequisites>
      <li>PlantElementRefining</li>
    </researchPrerequisites>
    <recipeUsers>
      <li>PlantReactionTable</li>
    </recipeUsers>
  </RecipeDef>
  <RecipeDef>
    <defName>Make_PlantModule</defName>
    <label>制作植模块</label>
    <description>制作植模块，第二遍了。</description>
    <jobString>正在制作植模块。</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>5000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>PlantElement</li>
          </thingDefs>
        </filter>
        <count>6</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <PlantModule>1</PlantModule>
    </products>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <researchPrerequisites>
      <li>PlantModuleMakeing</li>
    </researchPrerequisites>
    <recipeUsers>
      <li>PlantReactionTable</li>
    </recipeUsers>
  </RecipeDef>
</Defs>