<?xml version="1.0" encoding="utf-8"?>
<Defs>
  
  <HediffDef ParentName = "PsycastHediffBase">
    <defName>FleshSeedParasitized</defName>
    <label>血肉种子寄生</label>
    <description>因遭到Sayers的触手攻击而受到的异常状态。死亡后，一棵“可食用的”血肉之树将会破体而出。</description>
    <descriptionHyperlinks>
      <AbilityDef>ParasiticStab_Sayers</AbilityDef>
	</descriptionHyperlinks>
    <isBad>true</isBad>
	<everCurableByItem>false</everCurableByItem>
    <stages>
      <li>
        <statFactors />
        <statOffsets />
      </li>
    </stages>
    <comps>
      <li Class="Explorite.HediffCompProperties_FleshSeedParasitized">
        <fleck>AcidCloud</fleck>
        <moteCount>6</moteCount>
        <moteOffsetRange>0.25~0.75</moteOffsetRange>
        <filth>Filth_Blood</filth>
        <sound>DeathAcidifier</sound>
        <injuryCreatedOnDeath IsNull="Ture" />
        <injuryCount>0~0</injuryCount>
      </li>
    </comps>
  </HediffDef>
  
</Defs>