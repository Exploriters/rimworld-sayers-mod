    <description>一副已同步的护目镜。</description>
    <descriptionHyperlinks>
      <ThingDef>Goggles_normal</ThingDef>
    </descriptionHyperlinks>
    <spawnThingOnRemoved>Goggles_normal</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.25</partEfficiency>
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
  </HediffDef>
  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
    <defName>InstallGoggles_normal</defName>
    <label>同步护目镜</label>
    <description>同步护目镜。</description>
    <descriptionHyperlinks>
      <ThingDef>Goggles_normal</ThingDef>
      <HediffDef>Goggles_normal</HediffDef>
    </descriptionHyperlinks>
    <jobString>正在同步护目镜</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Goggles_normal</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Goggles_normal</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Goggles</li>
    </appliedOnFixedBodyParts>
    <addsHediff>Goggles_normal</addsHediff>
  </RecipeDef>
