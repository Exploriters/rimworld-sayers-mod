      <RecipeDef>InstallGoggles_Hide</RecipeDef>
    </descriptionHyperlinks>
    <costList>
      <Plasteel>15</Plasteel>
      <ComponentSpacer>4</ComponentSpacer>
    </costList>
    <thingSetMakerTags>
